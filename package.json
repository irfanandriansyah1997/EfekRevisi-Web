{
  "name": "efekrevisiweb",
  "version": "0.0.0",
  "description": "Efek Revisi Web (SPA)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EfekRevisi/EfekRevisi-Web.git"
  },
  "keywords": [
    "Vue",
    "JS",
    "SPA"
  ],
  "pre-commit": [
    "precommit-msg",
    "precommit"
  ],
  "author": "Irfan Andriansyah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EfekRevisi/EfekRevisi-Web/issues"
  },
  "homepage": "https://github.com/EfekRevisi/EfekRevisi-Web#readme",
  "scripts": {
    "dev": "npm install && node_modules/.bin/webpack-dev-server --progress --config build/webpack.config.dev.js",
    "prod": "node_modules/.bin/webpack --config build/webpack.config.prod.js",
    "lint": "node_modules/.bin/eslint --ext .js,.vue src",
    "lint:fix": "node_modules/.bin/eslint --ext .js,.vue src --fix",
    "pretest": "npm run lint",
    "test": "node_modules/.bin/jest",
    "storybook": "npm install && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precommit": "docker-compose run --rm web npm run test"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-persist": "^1.4.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.3",
    "@storybook/addon-links": "^4.0.0-alpha.3",
    "@storybook/addons": "^4.0.0-alpha.3",
    "@storybook/vue": "^4.0.0-alpha.3",
    "@vue/test-utils": "^1.0.0-beta.19",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.4",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "codecov": "^3.0.4",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.4",
    "jest-canvas-mock": "^1.1.0",
    "jest-serializer-vue": "^2.0.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "pre-commit": "^1.2.2",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.2.4",
    "vue-server-renderer": "^2.5.16",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "jest": {
    "collectCoverage": true,
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/**/*.component.{js}",
      "!src/**/index.{js}",
      "!src/**/components/index.{js}",
      "!src/**/mixins/index.{js}",
      "!src/**/router/*.{js}",
      "!src/**/stories/*.{js,vue}"
    ],
    "coverageDirectory": ".jest_coverage",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ]
  }
}
