    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.4",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
